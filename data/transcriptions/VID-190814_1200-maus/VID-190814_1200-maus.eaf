        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a5"
                TIME_SLOT_REF1="ts7" TIME_SLOT_REF2="ts8">
                <ANNOTATION_VALUE>terô zastanavja sa, co z tõ skórõ zrobjic, vrzuca do kòsza, a kòsz nazad ta skóra woddôł, won, ta mesz nazad to vrzuci, a ten kòsz nazôd téż ta skóra woddôł</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a6"
                TIME_SLOT_REF1="ts9" TIME_SLOT_REF2="ts10">
                <ANNOTATION_VALUE>mesz jidze, zdrzi, co tam v tim kòszu je</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a7"
                TIME_SLOT_REF1="ts11" TIME_SLOT_REF2="ts12">
                <ANNOTATION_VALUE>je baro zdzevòvónô, vrzucô jesz rôz, wobrôcô... wobracivô na dregô strona ten kòsz, a ten kòsz jidze z tełu za tõ meszõ</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a8"
                TIME_SLOT_REF1="ts13" TIME_SLOT_REF2="ts14">
                <ANNOTATION_VALUE>pòd spódka beł zdôvô mje so, że jakjisz żłóv, ale nje jem gvesnô</ANNOTATION_VALUE>
